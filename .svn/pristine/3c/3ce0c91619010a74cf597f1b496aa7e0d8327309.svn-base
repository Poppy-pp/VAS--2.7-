/*高德地图样式 自定义点样式 start*/                 
div.userMaker span.name{
  color: #ff5722;
  width: 100%;
  text-align: center;
  display: inline-block;
}
div.userMaker div.droplet_me{
  width: 2.5em;
  height: 2.5em;
  position: relative;
  border-radius: 50%;
  background: #333333;
  float: left;
  box-shadow: inset -1px 1px 4px 2px #444, inset 1px -1px 4px 2px #222;
  -webkit-transition: all 1s;
  -moz-transition: all 1s;
  transition: all 1s;
  -webkit-animation: bs linear 2s 1.5s infinite;
  -moz-animation: bs linear 2s 1.5s infinite;
  animation: bs linear 2s 1.5s infinite;
}
@-webkit-keyframes bs {
  0% {
    box-shadow: inset -1px 1px 3px 2px #444444, inset 1px -1px 3px 2px #222222, 0 0 0px 0 red;
  }
  50% {
    box-shadow: inset -1px 1px 3px 2px #444444, inset 1px -1px 3px 2px #222222, 0 0 20px 0 #1affff;
  }
  100% {
    box-shadow: inset -1px 1px 3px 2px #444444, inset 1px -1px 3px 2px #222222, 0 0 0px 0 red;
  }
}
@keyframes bs {
  0% {
    box-shadow: inset -1px 1px 3px 2px #444444, inset 1px -1px 3px 2px #222222, 0 0 0px 0 red;
  }
  50% {
    box-shadow: inset -1px 1px 3px 2px #444444, inset 1px -1px 3px 2px #222222, 0 0 20px 0 #1affff;
  }
  100% {
      box-shadow: inset -1px 1px 3px 2px #444444, inset 1px -1px 3px 2px #222222, 0 0 0px 0 red;
  }
}


div.droplet {
  width: 2.5em;
  height: 2.5em;
  border-radius: 80% 0 55% 50% / 55% 0 80% 50%;
  background-color: #000;
  transform: rotate(135deg);
  overflow: hidden;
}
div.droplet img{
  width: 100%;
  height: 100%;
  transform: rotate(-135deg);
}

