.logistice_content article {
  position: relative;
  padding: 20px;
}
.logistice_content article > h3 {
  width: 15%;
  height: 20px;
  line-height: 20px;
  text-align: right;
  font-size: 1.4em;
  color: #1d1d1d;
  padding: 10px 0 20px;
}
.logistice_content article section {
  padding: 0 0 17px;
  position: relative;
}
.logistice_content article section:before {
  content: "";
  width: 5px;
  top: 17px;
  bottom: -17px;
  left: 86px;
  background: #e6e6e6;
  position: absolute;
}
.logistice_content article section:last-child:before {
  display: none;
}
.logistice_content article section time {
  width: 71px;
  display: block;
  position: absolute;
}
.logistice_content article section time > span {
  display: block;
  text-align: right;
}
.logistice_content article section div.aside {
  color: #3a3a38;
  margin-left: 110px;
  padding-bottom: 15px;
}
.logistice_content article section .brief {
  color: #9f9f9f;
}
