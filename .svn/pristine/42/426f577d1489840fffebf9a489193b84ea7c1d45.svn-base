/**天气**/
.weather-card{
  border-radius: 5px;
  position: relative;
  overflow: hidden;
  height: 210px;
  display: flex;
  box-shadow: 0 2px 4px 0 rgba(0, 0, 0, 0.12), 0 0 6px 0 rgba(0, 0, 0, 0.04);
  -webkit-animation: appear 500ms ease-out forwards;
  animation: appear 500ms ease-out forwards;
}
.weather-card div.message{
  display: flex;
  align-items: center;
  justify-content: center;
  height: 80px;
}
.weather-card div.message,.weather-card section{
  text-align: center;
}
.weather-card section{
  flex:1;
  line-height: 30px;
  font-size: 1.6rem;
  padding: 5px;
}
.weather-card section>div{
  position: relative;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
}
.weather-card section>div::after{
  content: ' ';
  width: 80%;
  height: 1px;
  background: #f5f5f5;
  position: absolute;
  left: 50%;
  bottom: 0;
  -webkit-transform:translateX(-50%);
}
.weather-card section>div:last-child::after{
  height: 0;
}
.weather-card div.tq{
  flex:0 0 150px;
  width: 150px;
}

