/**卡片样式**/
.cards {
  overflow: hidden;
  padding-bottom: 3px;
}
.cards .card {
  height: 100%;
  position: relative;
  width: 100%;
  cursor: pointer;
}
.cards .card .front {
  height: 100%;
  position: relative;
  width: 100%;
  background-color: #f2f2f2;
  padding: 14px 0 15px 15px;
  min-height: 100px;
  top: 0;
  left: 0;
  z-index: 900;
  border-radius: 4px;
  -webkit-box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
  box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
  -webkit-transform: rotateX(0deg) rotateY(0deg);
  -ms-transform: rotateX(0deg) rotateY(0deg);
  transform: rotateX(0deg) rotateY(0deg);
  -webkit-transform-style: preserve-3d;
  -ms-transform-style: preserve-3d;
  transform-style: preserve-3d;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-transition: all 0.4s ease-in-out;
  transition: all 0.4s ease-in-out;
}
.cards .card .front .media {
  margin-bottom: 0;
  display: flex;
}
.cards .card .front .media span {
  font-size: 3rem;
  padding: 15px 15px 0 0;
  flex: 0 0 35px;
}
.cards .card .front .media .media-body{
  white-space: nowrap;
  line-height: 35px;
  flex:1;
}
.cards .card .front .media .media-body h2 {
  font-size: 2.4rem;
  white-space: nowrap;
}
.cards .card .front .media .media-body small {
  font-size: 1.6rem;
  color: #ffffff;
  white-space: nowrap;
}
.cards .card .back {
  position: absolute;
  top: 0;
  background-color: white !important;
  border: 0;
  color: inherit !important;
  padding: 12px;
  text-align: center;
  height: auto;
  width: 100%;
  min-height: 100px;
  left: 0;
  z-index: 800;
  border-radius: 4px;
  -webkit-box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
  box-shadow: 0 2px 2px rgba(0, 0, 0, 0.25);
  -webkit-transform: rotateY(-179deg);
  -ms-transform: rotateY(-179deg);
  transform: rotateY(-179deg);
  -webkit-transform-style: preserve-3d;
  -ms-transform-style: preserve-3d;
  transform-style: preserve-3d;
  -webkit-backface-visibility: hidden;
  backface-visibility: hidden;
  -webkit-transition: all 0.3s ease-in-out;
  transition: all 0.3s ease-in-out;
}
.cards .card .back > a {
  display: block;
  color: #666666;
  font-size: 1.6rem;
  font-weight: 300;
  line-height: 30px;
  text-decoration: none;
}
.cards .card .back > a > span:nth-of-type(1) {
  display: block;
  margin-bottom: 4px;
  font-size: 2rem;
}
.cards .card .back > a:hover {
  text-decoration: none;
  color: #1bc3fb;
}
.cards .card.flip .front {
  z-index: 900;
  -webkit-transform: rotateY(180deg);
  -ms-transform: rotateY(180deg);
  transform: rotateY(180deg);
}
.cards .card.flip .back {
  z-index: 1000;
  -webkit-transform: rotateX(0deg) rotateY(0deg);
  -ms-transform: rotateX(0deg) rotateY(0deg);
  transform: rotateX(0deg) rotateY(0deg);
}
.cards .card.card-orange .front {
  color: white;
  background-color: #9c28b1;
}
.cards .card.card-orange .back p {
  color: #FE8661;
}
.cards .card.card-blue .front {
  color: white;
  background-color: #de445e;
}
.cards .card.card-blue .back p {
  color: #56BDDE;
}
.cards .card.card-purple .front {
